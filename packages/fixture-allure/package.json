{
  "name": "@playwright-labs/fixture-allure",
  "version": "1.0.2",
  "description": "Allure fixture for Playwright",
  "main": "dist/index.cjs",
  "types": "dist/index.d.ts",
  "type": "module",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "tsup"
  },
  "keywords": [
    "playwright",
    "fixture",
    "allure",
    "automation",
    "testing",
    "fixture",
