  "packageManager": "pnpm@10.11.0",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.mts",
        "default": "./dist/index.mjs"
      },
      "require": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.cjs"
      }
    }
  },
  "devDependencies": {
    "@playwright/test": "1.57.0",
    "allure-js-commons": "3.4.5"
  },
  "peerDependencies": {
    "@playwright/test": "1.57.0",
    "allure-js-commons": "3.4.5"
  },
  "dependencies": {
    "@rslike/std": "3.2.0"
  }
