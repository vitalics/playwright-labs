{
  "name": "@playwright-labs/fixture-timers",
  "version": "1.0.0",
  "description": "Timers nodejs api support for Playwright",
  "main": "dist/index.cjs",
  "types": "dist/index.d.ts",
  "type": "module",
  "scripts": {
    "test": "playwright test",
    "build": "tsup",
    "ci:pretest": "playwright install",
    "package:pack": "pnpm exec npm pack",
    "package:pack:dry-run": "pnpm package:pack --dry-run"
  },
  "keywords": [
    "playwright",
    "automation",
    "testing",
    "playwright-labs",
    "decorators"
  ],
  "author": "Vitali Haradkou <vitalicset@yandex.ru>",
  "license": "MIT",
  "packageManager": "pnpm@10.11.0",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.mts",
        "default": "./dist/index.mjs"
      },
      "require": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.cjs"
      }
    }
  },
  "devDependencies": {
    "@playwright/test": "1.57.0"
  },
  "peerDependencies": {
    "@playwright/test": "1.57.0"
  },
  "dependencies": {}
}
