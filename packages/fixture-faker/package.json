{
  "name": "@playwright-labs/fixture-faker",
  "version": "1.0.2",
  "description": "Faker fixture integration for Playwright",
  "main": "dist/index.cjs",
  "types": "dist/index.d.ts",
  "type": "module",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 0",
    "build": "tsup",
    "package:pack": "pnpm exec npm pack",
    "package:pack:dry-run": "pnpm package:pack --dry-run"
  },
  "keywords": [
    "playwright",
    "fixture",
    "faker",
    "fake",
    "fake-data",
    "data",
    "mocking",
    "automation",
    "testing",
    "fixture",
    "playwright-labs"
  ],
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.mts",
        "default": "./dist/index.mjs"
      },
      "require": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.cjs"
      }
    }
  },
  "author": "Vitali Haradkou <vitalicset@yandex.ru>",
  "license": "MIT",
  "packageManager": "pnpm@10.11.0",
  "devDependencies": {
    "@faker-js/faker": "10.2.0",
    "@playwright/test": "1.57.0"
  },
  "peerDependencies": {
    "@faker-js/faker": "10.2.0",
    "@playwright/test": "1.57.0"
  },
  "dependencies": {
    "@rslike/std": "3.2.0"
  }
}
