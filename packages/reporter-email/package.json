{
  "name": "@playwright-labs/reporter-email",
  "version": "1.0.2",
  "description": "Email reporter for Playwright",
  "main": "dist/index.cjs",
  "types": "dist/index.d.ts",
  "type": "module",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 0",
    "build": "tsup",
    "package:pack": "pnpm exec npm pack",
    "package:pack:dry-run": "pnpm package:pack --dry-run"
  },
  "keywords": [
    "playwright",
    "automation",
    "testing",
    "playwright-labs",
    "reporter",
    "email",
    "notification",
    "nodemailer",
    "email-reporter"
  ],
  "author": "Vitali Haradkou <vitalicset@yandex.ru>",
  "license": "MIT",
  "packageManager": "pnpm@10.11.0",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.mts",
        "default": "./dist/index.mjs"
      },
      "require": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.cjs"
      }
    }
  },
  "devDependencies": {
    "@playwright/test": "1.57.0",
    "@types/nodemailer": "7.0.9"
  },
  "peerDependencies": {
    "@playwright/test": "1.57.0"
  },
  "dependencies": {
    "nodemailer": "7.0.13"
  }
}
